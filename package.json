{
  "name": "unified-customer-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:a11y": "jest __tests__/a11y",
    "test:theme": "jest __tests__/theme",
    "test:security": "jest __tests__/security",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headless": "cypress run --headless",
    "test:webhook": "tsx scripts/test-dwolla-webhook.ts",
    "test:e2e:ci": "start-server-and-test dev http://localhost:3000 'cypress run --headless --record'",
    "test:regression": "cypress run --spec 'cypress/e2e/regression-suite.cy.ts,cypress/e2e/week-preset-date-range.cy.ts,cypress/e2e/custom-date-range.cy.ts,cypress/e2e/status-badge.cy.ts,cypress/e2e/pagination.cy.ts'",
    "check:env": "node scripts/check-env.js",
    "setup": "npm install && npm run check:env && npx prisma generate",
    "setup:db": "npx prisma migrate dev && npx prisma db seed",
    "setup:demo": "node scripts/setup-demo.js",
    "seed:ach": "ts-node --project tsconfig.scripts.json scripts/seed-ach-transactions.ts",
    "webhook:test": "tsx scripts/test-webhook-sync.ts",
    "webhook:setup": "tsx scripts/setup-webhook-subscription.ts",
    "webhook:system:test": "tsx scripts/test-webhook-system.ts",
    "webhook:worker": "tsx scripts/webhook-worker.ts",
    "webhook:worker:dev": "nodemon --watch 'lib/webhooks/**/*.ts' --exec 'tsx scripts/webhook-worker.ts'",
    "ach:sync:all": "tsx scripts/sync-all-ach-transactions.ts",
    "ach:sync:worker": "tsx scripts/ach-sync-worker.ts",
    "ach:sync:worker:dev": "nodemon --watch 'lib/api/dwolla/**/*.ts' --exec 'tsx scripts/ach-sync-worker.ts'",
    "ach:cleanup:debits": "tsx scripts/cleanup-debit-transactions.ts",
    "ach:cleanup:test": "tsx scripts/cleanup-test-transactions.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run webhook:worker\" \"npm run ach:sync:worker\""
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.525.0",
    "next": "15.4.1",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "prisma": "^6.12.0",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "19.1.0",
    "recharts": "^3.1.0",
    "sonner": "^2.0.6",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@swc/jest": "^0.2.39",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "commander": "^12.0.0",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.2",
    "cypress-real-events": "^1.14.0",
    "dotenv": "^17.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "eslint-config-prettier": "^10.1.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "start-server-and-test": "^2.0.12",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
