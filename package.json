    "@types/speakeasy": "^2.0.10",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.525.0",
